{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
   "paths": {
  "@/*": ["../slack-tutorials/src/*"],
  "@components/*": ["../slack-tutorials/src/components/*"]
}
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../slack-tutorial/.next/types/**/*.ts",
    "../slack-tutorial/.next/types/**/*.ts",
    "../slack-tutorial/next.config.ts",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
